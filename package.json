{
	"name": "react-native-animated-layout",
	"description": "An absolute responsive layout for react-native and react-native-web projects.",
	"version": "1.0.17",
	"private": false,
	"author": "MehmetKaplan",
	"license": "MIT",
	"repository": "MehmetKaplan/react-native-animated-layout",
	"main": "dist/module/index.js",
	"module": "dist/module/index.js",
	"react-native": "src/index.js",
	"files": [
		"src",
		"dist"
	],
	"scripts": {
		"prepare": "bob build",
		"gh-pages": "cd example && npx expo build:web && cd - && git add -A && git commit -m \"github page deployment\" --quiet && git push && gh-pages -d example/web-build"
	},
	"peerDependencies": {
		"react": "*",
		"react-dom": "*",
		"react-native": "*",
		"react-native-reanimated": "^3.0.0",
		"react-native-web": "*"
	},
	"dependencies": {},
	"devDependencies": {
		"@react-native-community/bob": "^0.17.1",
		"gh-pages": "^4.0.0",
		"metro-react-native-babel-preset": "^0.71.1"
	},
	"@react-native-community/bob": {
		"source": "src",
		"output": "dist",
		"targets": [
			"module"
		]
	},
	"keywords": [
		"react-native",
		"react-native-web",
		"responsive",
		"layout",
		"absolute"
	]
}
